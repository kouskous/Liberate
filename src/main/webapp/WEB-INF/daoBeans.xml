<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:context="http://www.springframework.org/schema/context"
       xmlns:mvc="http://www.springframework.org/schema/mvc"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-4.0.xsd">
    
    <bean id="FichierUserDao" class="dao.FichierUserDao" scope="singleton">
        <constructor-arg index="0" type="java.lang.String" value="persistenceUnitLiber8"/>
    </bean>
    
    <bean id="ProjetDao" class="dao.ProjetDao" scope="singleton">
        <constructor-arg index="0" type="java.lang.String" value="persistenceUnitLiber8"/>
    </bean>
    
    <bean id="UserDao" class="dao.UserDao" scope="singleton">
        <constructor-arg index="0" type="java.lang.String" value="persistenceUnitLiber8"/>
    </bean>
    
    <bean id="UserProjetDao" class="dao.UserProjetDao" scope="singleton">
        <constructor-arg index="0" type="java.lang.String" value="persistenceUnitLiber8"/>
    </bean>
    
</beans>
